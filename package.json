{
  "name": "ai-page-summarizer",
  "version": "1.0.0",
  "description": "Chrome extension that summarizes webpage content using AI",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css}'",
    "format:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css}'",
    "fix:all": "npm run format:fix && npm run lint:fix",
    "analyze:unused": "ts-prune",
    "analyze:deps": "depcheck",
    "analyze:exports": "ts-unused-exports tsconfig.json --excludePathsFromReport=dist",
    "analyze:knip": "knip",
    "analyze:all": "npm run analyze:knip && npm run analyze:deps && npm run analyze:unused",
    "analyze:fix": "npm run lint:fix && npm run format:fix"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/chrome": "^0.0.270",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "autoprefixer": "^10.4.16",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.8.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "knip": "^5.63.0",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.6.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "ts-loader": "^9.5.1",
    "ts-prune": "^0.10.3",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^5.3.3",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "marked": "^16.1.2",
    "portkey-ai": "^1.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
